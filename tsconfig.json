{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "rootDir": "src",
    "outDir": "dist",
    "removeComments": false
  }
}
